		"url": "https://github.com/anubhavsrivastava/react-for-electron-only/issues"
	},
	"homepage": "https://github.com/anubhavsrivastava/react-for-electron-only#readme",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
